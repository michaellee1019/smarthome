{
  "extends": "gts/tslint.json",
  "rules": {
    "ordered-imports": [
      true,
      {
        "grouped-imports": true
      }
    ],
    "indent": ["spaces", 4],
    "no-any": false,
    "no-duplicate-imports": true
  },
  "linterOptions": {
    "exclude": [
      "**/*.json",
      "**/*.js",
      "**/*.compiled.js"
    ]
  }
}
